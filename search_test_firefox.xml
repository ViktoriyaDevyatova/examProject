<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Search tests on Firefox browser" verbose="1" >
    <test name="Nopackage" >
        <parameter name="browserType" value="firefox"/>
        <parameter name="url" value="https://www.google.com.ua/"/>
        <classes>
            <class name="tests.GoogleSearchTest" />
        </classes>
    </test>
</suite>